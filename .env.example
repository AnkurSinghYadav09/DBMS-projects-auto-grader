# Example .env file - Copy to .env and fill in your values

# AI Provider Configuration
AI_PROVIDER=gemini
# Options: openai, gemini, deepseek

# Gemini Configuration (Free tier)
GEMINI_API_KEY=your_gemini_api_key_here
GEMINI_MODEL=models/gemini-2.5-flash

# Google Sheet Configuration
SPREADSHEET_ID=your_google_sheet_id_here
SHEET_NAME=Sheet1

# Column Configuration (A=1, B=2, etc.)
DOC_LINK_COLUMN=A
STUDENT_NAME_COLUMN=B
SCORE_COLUMN=C
FEEDBACK_COLUMN=D

# Service Account
SERVICE_ACCOUNT_FILE=credentials/service_account.json

# Processing Settings
MAX_WORKERS=5
RETRY_ATTEMPTS=3
BATCH_SIZE=10

# Features
INCLUDE_PLAGIARISM_CHECK=True
